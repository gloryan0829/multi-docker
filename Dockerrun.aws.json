{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "esak248/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 3000
        }
      ]
    },{
      "name": "server",
      "image": "esak248/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 5000
        }
      ]
    },{
      "name": "worker",
      "image": "esak248/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },{
      "name": "nginx",
      "image": "esak248/multi-nginx",
      "hostname": "ngnix",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80 ,
          "containerPort": 80
        }
      ],
      "links": [
        "client", "server"
      ],
      "memory": 128
    }
  ]
}